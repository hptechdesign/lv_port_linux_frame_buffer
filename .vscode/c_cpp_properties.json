{
    "configurations": [
        {
            "name": "RPI-Win",
            "includePath": [
                "${workspaceRoot}/**",
                "${workspaceRoot}/src/**",
                "${workspaceRoot}/inc/**",
                "${RPIDEV_LOC}/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "compilerPath": "",
            "intelliSenseMode": "clang-x64",
            "browse": {
                "path": [
                    "${workspaceRoot}",
                    "${RPIDEV_LOC}/**"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": "${workspaceRoot}/.vscode/browsedb/browse.vc."
            },
            "configurationProvider": "ms-vscode.makefile-tools"
        }
    ],
    "version": 4
}